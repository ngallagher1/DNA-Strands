import java.io.*;
import java.util.*;
import java.lang.*;
import java.text.*;

public class DNAPairsHW
{

  public static void main(String[] args)
  {
	  StringBuilder pairs = new StringBuilder();
	  String regex = "CGTA";
	  int length = 6;
	  String dna = createListOfPairs(pairs, regex, length);
	  System.out.print(dna);
  }

  public static String createListOfPairs(StringBuilder p, String regex, int length)
  {
	  StringBuilder temp = new StringBuilder();
	  temp.append(p);
	  if(length == 0)
	  	System.out.println(temp);
	  else
	  {
	  	for(int i = 0; i < 4; i++)
	  	{
			createListOfPairs(temp.append(regex.charAt(i)), regex, length-1);

		}
	  }
	  return p.toString();

	  /*for(int n = 0; n < Math.pow(4, length); n++)
	  {
		  StringBuilder s = new StringBuilder();
		  for(int k = 0; k < 6; k++)
	  	  {
			  s.append(regex.charAt());
	  	  }
	  	  p.append(s);
	  }
	  String string = p.toString();
	  return string;
	  */
  }
